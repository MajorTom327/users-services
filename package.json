{
  "name": "users-services",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build": "nodemon --watch ./src --watch ./scripts --ext ts,mjs --exec npm run build",
    "dev:server": "nodemon --watch ./dist --ext js --exec node ./dist/index.js",
    "test": "vitest --config ./config/vitest.config.mjs --coverage",
    "test:ci": "vitest --config ./config/vitest.config.mjs --coverage run",
    "lint": "eslint --fix --ext .ts ."
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.38",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/luxon": "^3.2.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/ramda": "^0.28.21",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@vitest/coverage-c8": "^0.28.1",
    "esbuild": "^0.17.4",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.1"
  },
  "dependencies": {
    "@apollo/server": "^4.3.1",
    "@prisma/client": "4.9.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "luxon": "^3.2.1",
    "morgan": "^1.10.0",
    "ramda": "^0.28.0",
    "ramda-adjunct": "^3.4.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
